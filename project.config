[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group ZTE Axon7 Maintainers
	owner = group user/AOSNP-Gerrit
[access "refs/for/refs/*"]
	read = group ZTE Axon7 Maintainers
	read = group user/AOSNP-Gerrit
	push = group ZTE Axon7 Maintainers
	push = group user/AOSNP-Gerrit
	label-Code-Review = -2..+2 group ZTE Axon7 Maintainers
	label-Code-Review = -2..+2 group user/AOSNP-Gerrit
	label-Verified = -1..+1 group ZTE Axon7 Maintainers
	label-Verified = -1..+1 group user/AOSNP-Gerrit
	rebase = group ZTE Axon7 Maintainers
	rebase = group user/AOSNP-Gerrit
	removeReviewer = group ZTE Axon7 Maintainers
	removeReviewer = group user/AOSNP-Gerrit
	submit = group ZTE Axon7 Maintainers
	submit = group user/AOSNP-Gerrit
[access "refs/heads/*"]
	read = group ZTE Axon7 Maintainers
	read = group user/AOSNP-Gerrit
	create = group ZTE Axon7 Maintainers
	create = group user/AOSNP-Gerrit
	push = +force group ZTE Axon7 Maintainers
	push = +force group user/AOSNP-Gerrit
	pushMerge = group ZTE Axon7 Maintainers
	pushMerge = group user/AOSNP-Gerrit
[access "refs/tags/*"]
	push = +force group ZTE Axon7 Maintainers
	push = group user/AOSNP-Gerrit
	push = +force group user/AOSNP-Gerrit
[submit]
	action = rebase if necessary
